version: '3'
services:
  web:
    build: .
    ports:
     - "5000:5000"
    volumes:
     - .:/code
  redis:
    image: "redis:alpine"    
  mysql:
    environment:
      MYSQL_ROOT_PASSWORD: "123456"
    image: mysql
    restart: unless-stopped
    privileged:true 
    volumes:
      - "/usr/local/mysql_master/data:/var/lib/mysql"
      - "/usr/local/mysql_master/conf:/etc/mysql/conf.d"